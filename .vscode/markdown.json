{
    // Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and
    // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
    // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
    // same ids are connected.
    // Example:
    // "Print to console": {
    // 	"prefix": "log",
    // 	"body": [
    // 		"console.log('$1');",
    // 		"$2"
    // 	],
    // 	"description": "Log output to console"
    // }
    "Link": {
        "prefix": ".addlink",
        "body": [
            "[${TM_SELECTED_TEXT}]($2)",
            "",
            "$0"
        ],
        "description": "Insert a link"
    },
    "ConvertLink": {
        "prefix": ".linktitle",
        "body": [
            "[$1](${TM_SELECTED_TEXT})",
            "",
            "$0"
        ],
        "description": "Take a site and make it into a link"
    },
    "SelectToLink": {
        "prefix": ".selectedTextToHyperlink",
        "body": [
            "[${1:${TM_SELECTED_TEXT}}](${CLIPBOARD})",
            "",
            "$0"
        ],
        "description": "Take a site and make it into a link"
    },
    "ClipboardLink": {
        "prefix": ".clipLink",
        "body": [
            "[$(1:${TM_SELECTED_TEXT})](${CLIPBOARD})",
            "",
            "$0"
        ],
        "description": "Take a site and make it into a link"
    },
    "_Code Block": {
        "prefix": "cb",
        "body": [
            "",
            "```${lang:LangName}",
            "$1",
            "```",
            "",
            "$0"
        ],
        "description": "Produce a Code Block"
    },
    "_Code Block with Choices": {
        "prefix": "_CodeBlockSelectLanguage",
        "body": [
            "",
            "```${1|java,python,cmd,bash,R,js,json,C#,ps1,py3,py2,css|}",
            "$2",
            "```",
            "",
            "$0"
        ],
        "description": "Produce a Code Block"
    },
    "Paste Code": {
        "prefix": "code-clip",
        "body": [
            "",
            "```${TM_FILENAME_BASE/(.*)/${1:/downcase}/}",
            "${LINE_COMMENT}$1",
            "${CLIPBOARD}",
            "```",
            "",
            "$0"
        ],
        "description": "Produce a Code Block"
    },
    "Code From Selected": {
        "prefix": "_ConvertSelectedToCodeBlock",
        "body": [
            "```${TM_FILENAME_BASE/(.*)/${1:/downcase}/}",
            "${TM_SELECTED_TEXT}",
            "```",
            "",
            "$0"
        ],
        "description": "Converts the selected text to a code block."
    },
    "Link from Clipboard": {
        "prefix": "_link-clip",
        "body": [
            // "[${1:${TM_SELECTED_TEXT}}](${2:${CLIPBOARD:?{^{http:,ftp:,file:}$3})",
            "[${1:${TM_SELECTED_TEXT}}](${2:${CLIPBOARD}})",
            "",
            "$0"
        ],
        "description": "insert a link from the clipboard"
    },
    "test Choices (Not working)": {
        "prefix": "test",
        "body": [
            "${1| red, blue, green |}"
        ],
        "description": "Test Choices"
    },
    "Admonition": {
        "prefix": "!ad",
        "body": [
            "!!!${1|Note,Summary, Abstract, Tldr,Info, Todo,Tip, Hint,Success, Check, Done,Question, Help, Faq,Warning, Caution,Attention,Failure, Fail, Missing,Error,Bug,Danger,Example, Snippet,Quote, Cite|} ${1}: $2",
            "\t$3",
            "",
            "$0"
        ],
        "description": "Note (Blue Pencil), Summary/Abstract/Tldr (Lt. Blue Paragraph), Todo/Info (Light Blue circled i), Tip/Hint (Teal Flame), Success/Check/Done (Green Check), Question/Help/FAQ (Green Question Mark), Warning/Caution (Orange Triange Exclaim Mark), Attention (Orange Eye Exclaim Mark), Red Bug, Danger- Red Lightning Admonition, Failure, Fail, Missing, Error (Red X), Example, Snippet (Num List Blue), Quote/Cite (Grey Quote)"
    },
